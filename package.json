{
  "name": "eve-trader",
  "version": "1.0.0",
  "description": "Integrated with Horizon, Redux, Koa, ES6/ES7, JSX, Webpack, Babel, Material Design Lite, and PostCSS",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "deploy": "rollup -c rollup.prod.config.js && cp client/assets/** build/assets/** && cp client/index.html build/index.html && NODE_ENV=production babel-node server/index.js",
    "lint": "eslint --ignore-path .gitignore client server",
    "build-server": "babel server -d build-server",
    "serve": "node build-server/index.js",
    "dev": "rollup -c rollup.config.js --watch"
  },
  "pre-commit": "lint",
  "keywords": [
    "react",
    "es7",
    "jsx",
    "webpack",
    "babel",
    "postcss",
    "scaffolding",
    "fullstack"
  ],
