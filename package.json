{
  "name": "eve-trader",
  "version": "1.0.0",
  "description": "Integrated with Horizon, Redux, Koa, ES6/ES7, JSX, Webpack, Babel, Material Design Lite, and PostCSS",
  "scripts": {
    "start": "nodemon --watch 'server/**/*.js' --exec \"babel-node server/index.js\"",
    "deploy": "NODE_ENV=production webpack --config webpack.production.config.js && NODE_ENV=production babel-node server/index.js",
    "lint": "eslint --ignore-path .gitignore client server",
    "build-server": "babel server -d build-server",
    "serve": "node build-server/index.js"
  },
  "pre-commit": "lint",
  "keywords": [
    "react",
    "graphql",
    "express",
    "es6",
    "es7",
    "jsx",
    "webpack",
    "babel",
    "material design lite",
    "postcss",
    "scaffolding",
    "fullstack"
  ],
